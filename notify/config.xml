<?xml version='1.0' encoding='utf-8'?>
<widget id="com.speednotify.android" version="0.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Speed Notify 0.1.1404</name>
    <description>
        Speed Notify is an application of
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Speedemployer Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="www/img/logo.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="phonegap-plugin-push" spec="~2.1.3">
        <variable name="SENDER_ID" value="818483830718" />
    </plugin>
    <engine name="android" spec="^7.1.0" />
</widget>
